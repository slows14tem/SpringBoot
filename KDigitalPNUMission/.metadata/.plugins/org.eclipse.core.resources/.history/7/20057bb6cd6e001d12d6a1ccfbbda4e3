package com.pnu.dao;

import java.sql.Connection;
import java.sql.DriverManager;

public class BoardDAO {
	private Connection con = null;
	
	public BoardDAO() {
		try {
			Class.forName("org.h2.Driver");
			con = DriverManager.getConnection("jdbc:h2:tcp://localhost/~/mvcboard", "sa", "");
		} catch (Exception e) {
			e.printStackTrace();
		}
		
	}

}
