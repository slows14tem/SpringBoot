package edu.pnu.domain;

import java.util.Date;

import javax.persistence.Entity;

@Entity
public class Reply {
	
	private Long seq;
	private Board bSeq;
	private Member member;
	private Date createDate;
	
	public Reply() {
		// TODO Auto-generated constructor stub
	}

	public Reply(Long seq, Board bSeq, Member member, Date createDate) {
		super();
		this.seq = seq;
		this.bSeq = bSeq;
		this.member = member;
		this.createDate = createDate;
	}

	@Override
	public String toString() {
		return "Reply [seq=" + seq + ", bSeq=" + bSeq + ", member=" + member + ", createDate=" + createDate + "]";
	}

	public Long getSeq() {
		return seq;
	}

	public void setSeq(Long seq) {
		this.seq = seq;
	}

	public Board getbSeq() {
		return bSeq;
	}

	public void setbSeq(Board bSeq) {
		this.bSeq = bSeq;
	}

	public Member getMember() {
		return member;
	}

	public void setMember(Member member) {
		this.member = member;
	}

	public Date getCreateDate() {
		return createDate;
	}

	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}
	

}
